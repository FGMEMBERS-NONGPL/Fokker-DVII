<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <path>wingcompass.ac</path>

  <animation>
    <!-- Objets opaques -->
    <object-name>base</object-name>
    <object-name>rosette</object-name>
  
    <!-- Objets transparents -->
    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>noshadow</type>
    <object-name>vitre</object-name>
  </animation>

 
  <animation>
    <type>rotate</type>
    <object-name>rosette</object-name>
    <property>/instrumentation/heading-indicator/indicated-heading-deg</property>
    <axis>
      <x> 0.0 </x>
      <y> 0.0 </y>
      <z> 1.0 </z>
    </axis>
  </animation>

</PropertyList>

